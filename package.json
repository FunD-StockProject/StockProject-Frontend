    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-emotion-order": "^1.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "svgo": "^4.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.21.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-plugin-wasm": "^3.4.1"
